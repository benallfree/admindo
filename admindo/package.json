{
  "name": "admindo",
  "version": "0.0.1-rc.2",
  "type": "module",
  "description": "Zero-dependency admin dashboard for Cloudflare Durable Objects",
  "files": [
    "dist"
  ],
  "exports": {
    "./vanilla": {
      "default": "./dist/vanilla.mjs",
      "import": "./dist/vanilla.mjs",
      "types": "./dist/vanilla.d.ts"
    },
    "./hono": {
      "default": "./dist/hono.mjs",
      "import": "./dist/hono.mjs",
      "types": "./dist/hono.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "admin",
    "dashboard",
    "spa",
    "web-components",
    "pluggable",
    "esm"
  ],
  "authors": [
    {
      "name": "Ben Allfree",
      "url": "https://x.com/benallfree"
    }
  ],
  "license": "MIT",
  "peerDependencies": {
    "hono": "^4.7.10"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
